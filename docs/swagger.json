        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/HoldingDto"
        }
      },
      "Ticker" : {
        "type" : "object",
        "properties" : {
          "isin" : {
            "type" : "string"
          },
          "symbol" : {
            "type" : "string"
          },
          "securityType" : {
            "type" : "string"
          },
          "companyName" : {
            "type" : "string"
          }
        }
      },
      "TickerDto" : {
        "type" : "object",
        "properties" : {
          "symbol" : {
            "type" : "string"
          },
          "companyName" : {
            "type" : "string"
          },
          "securityType" : {
            "type" : "string"
          }
        }
      },
      "TransactionsDto" : {
        "type" : "object",
        "properties" : {
          "date" : {
            "$ref" : "#/components/schemas/LocalDate"
          },
          "ticker" : {
            "$ref" : "#/components/schemas/Ticker"
          },
          "quantity" : {
            "type" : "integer",
            "format" : "int32"
          },
          "cost" : {
            "type" : "number"
          }
        }
      }
